<h1>Branch Logs</h1>

<table class="responsive nowrap table table-hover" id="dttb-branch_logs" data-sort="true" data-source="<%= url_for(format: :json)%>">
  <thead>
    <tr>
      <th data-data="branch_id">Branch</th>
      <th data-data="user_id">User</th>
      <th data-data="name">Name</th>
      <th data-data="address">Address</th>
      <th data-data="note">Note</th>
      <th data-data="url" data-orderable="false" data-class-name="all" data-searchable="false" class="skip-export" width="100px"></th>
    </tr>
  </thead>

</table>

<br>

<%= link_to 'New Branch Log', new_branch_log_path %>
