<h1>Dollar Logs</h1>

<table class="responsive nowrap table table-hover" id="dttb-dollar_logs" data-sort="true" data-source="<%= url_for(format: :json)%>">
  <thead>
    <tr>
      <th data-data="dollar_id">Dollar</th>
      <th data-data="user_id">User</th>
      <th data-data="note">Note</th>
      <th data-data="rate">Rate</th>
      <th data-data="url" data-orderable="false" data-class-name="all" data-searchable="false" class="skip-export" width="100px"></th>
    </tr>
  </thead>

</table>

<br>

<%= link_to 'New Dollar Log', new_dollar_log_path %>